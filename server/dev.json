{
  "implementation": "dfsp-ledger",
  "console": {
    "port": 30014,
    "server": {
      "host": "0.0.0.0",
      "port": 30014
    }
  },
  "httpserver": {
    "baseUrl": "http://localhost:8014/ledger"
  },
  "stdOut": {
    "mode": "dev"
  },
  "log": {
    "streams": [{
      "level": "trace",
      "stream": "../logRotateStream",
      "streamConfig": {
        "path": "ut5_%Y-%m-%d.log",
        "symlink": "ut5.log",
        "compress": true
      }
    }]
  },
  "masterBus": {
    "logLevel": "debug",
    "socket": "dfsp-ledger"
  },
  "workerBus": {
    "logLevel": "debug"
  },
  "db": {
    "logLevel": "trace",
    "db": {
      "host": "postgres"
    },
    "permission": {
      "requireSession": true
    }
  },
  "script": {
    "#mock": true
  },
  "debug": true
}
